<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service public="false" id="knp_doctrine_behaviors.user_provider" class="Knp\DoctrineBehaviors\Provider\UserProvider">
            <argument type="service" id="security.helper" />
            <argument /> <!-- replaced by knp_doctrine_behaviors.blameable.user_entity -->
        </service>
        <service public="false" id="knp_doctrine_behaviors.locale_provider" class="Knp\DoctrineBehaviors\Provider\LocaleProvider">
            <argument type="service" id="request_stack" />
            <argument type="service" id="parameter_bag" />
            <argument type="service" id="translator"/>
        </service>

        <service public="false" id="knp_doctrine_behaviors.repository.default_sluggable" class="Knp\DoctrineBehaviors\Repository\DefaultSluggableRepository">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>

        <service public="false" id="knp_doctrine_behaviors.event_subscriber.blameable" class="Knp\DoctrineBehaviors\EventSubscriber\BlameableSubscriber">
            <argument type="service" id="knp_doctrine_behaviors.user_provider" />
            <argument type="service" id="doctrine.orm.default_entity_manager" />
            <argument /> <!-- replaced by knp_doctrine_behaviors.blameable.user_entity -->
            <tag name="doctrine.event_subscriber" />
        </service>
        <service public="false" id="knp_doctrine_behaviors.event_subscriber.loggable" class="Knp\DoctrineBehaviors\EventSubscriber\LoggableSubscriber">
            <argument type="service" id="logger" />
            <tag name="doctrine.event_subscriber" />
        </service>
        <service public="false" id="knp_doctrine_behaviors.event_subscriber.sluggable" class="Knp\DoctrineBehaviors\EventSubscriber\SluggableSubscriber">
            <argument type="service" id="doctrine.orm.default_entity_manager" />
            <argument type="service" id="knp_doctrine_behaviors.repository.default_sluggable" />
            <tag name="doctrine.event_subscriber" />
        </service>
        <service public="false" id="knp_doctrine_behaviors.event_subscriber.soft_deleteable" class="Knp\DoctrineBehaviors\EventSubscriber\SoftDeletableSubscriber">
            <tag name="doctrine.event_subscriber" />
        </service>
        <service public="false" id="knp_doctrine_behaviors.event_subscriber.timestampable" class="Knp\DoctrineBehaviors\EventSubscriber\TimestampableSubscriber">
            <argument /> <!-- replaced by knp_doctrine_behaviors.timestampable.date_field_type -->
            <tag name="doctrine.event_subscriber" />
        </service>
        <service public="false" id="knp_doctrine_behaviors.event_subscriber.translatable" class="Knp\DoctrineBehaviors\EventSubscriber\TranslatableSubscriber">
            <argument type="service" id="knp_doctrine_behaviors.locale_provider" />
            <argument /> <!-- replaced by knp_doctrine_behaviors.translatable.translatable_fetch_mode -->
            <argument /> <!-- replaced by knp_doctrine_behaviors.translatable.translation_fetch_mode -->
            <tag name="doctrine.event_subscriber" />
        </service>
        <service public="false" id="knp_doctrine_behaviors.event_subscriber.tree" class="Knp\DoctrineBehaviors\EventSubscriber\TreeSubscriber">
            <tag name="doctrine.event_subscriber" />
        </service>
        <service public="false" id="knp_doctrine_behaviors.event_subscriber.uuidable" class="Knp\DoctrineBehaviors\EventSubscriber\UuidableSubscriber">
            <tag name="doctrine.event_subscriber" />
        </service>
    </services>
</container>
